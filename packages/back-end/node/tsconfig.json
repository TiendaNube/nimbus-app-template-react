{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "noEmit": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
